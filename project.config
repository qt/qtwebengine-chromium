[access]
	inheritFrom = All-Non-CI-Projects
[receive]
	requireContributorAgreement = true
	requireChangeId = true
[submit]
	action = cherry pick
	mergeContent = true
[project]
	description = ssh://codereview.qt-project.org:29418/qt-labs/chromium.git
[cherryPick]
	includeReviewedOn = true
[cherryPick "categoryFooters"]
	Sanity-Review = true
